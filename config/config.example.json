    "counters": {
        "descriptor": {
            "type": "log"
        },
        "options": {
            "timeout": 10000
        }
    },
    
    "persistence": {
        "descriptor": {
            "group": "pip-services-announces",            
            "type": "mongodb"
        },
        "connection": {
            "uri": "mongodb://localhost/pipservicestest"
        },
        "options": {
            "server": {
                "poolSize": 4,
                "socketOptions": {
                    "keepAlive": 1,
                    "connectTimeoutMS": 5000
                },
                "auto_reconnect": true
            },
            "debug": false        
        }
    },
